{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/website"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "website/**.html"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "toc.yml",
          "*.md",
          "*.html",
          "website/*.html"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "CNAME",
          "website/resources/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "templates/discordfx"
    ]
  }
}
